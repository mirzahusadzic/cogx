{
  "name": "cognition-cli",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "cognition-cli": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc && node build-worker.js",
    "build:worker": "node build-worker.js",
    "dev": "tsx watch src/cli.ts",
    "start": "node dist/cli.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts && markdownlint . --ignore node_modules",
    "format": "prettier --write \"**/*.{ts,js,json,md}\""
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "@lancedb/lancedb": "^0.22.2",
    "@types/chokidar": "^1.7.5",
    "@types/workerpool": "^6.4.7",
    "chalk": "^5.6.2",
    "chokidar": "^4.0.3",
    "commander": "^12.0.0",
    "dotenv": "^16.4.0",
    "esbuild": "^0.25.11",
    "execa": "^8.0.1",
    "form-data": "^4.0.0",
    "fs-extra": "^11.2.0",
    "js-yaml": "^4.1.0",
    "proper-lockfile": "^4.1.2",
    "remark": "^15.0.1",
    "remark-parse": "^11.0.0",
    "undici": "^7.16.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "upath": "^2.0.1",
    "workerpool": "^10.0.0",
    "zod": "^3.22.4"
