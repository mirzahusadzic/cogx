{
  "name": "cognition-cli",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "cognition-cli": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/cli.ts",
    "start": "node dist/cli.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts && markdownlint . --ignore node_modules",
    "format": "prettier --write \"**/*.{ts,js,json,md}\""
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.0",
    "execa": "^8.0.1",
    "form-data": "^4.0.0",
    "fs-extra": "^11.2.0",
    "js-yaml": "^4.1.0",
    "ofetch": "^1.3.3",
    "upath": "^2.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitepress": "^1.2.3",
    "vitest": "^1.6.0",
    "memfs": "^4.9.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.2",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "markdownlint-cli": "^0.41.0"
  }
}
