{
  "entryPoints": ["./src"],
  "entryPointStrategy": "expand",
  "out": "./docs/api",
  "exclude": ["**/*.test.ts", "**/__tests__/**", "**/node_modules/**"],
  "excludePrivate": false,
  "excludeProtected": false,
  "excludeInternal": true,
  "includeVersion": true,
  "readme": "docs/API_README.md",
  "name": "Cognition Î£ CLI - API Documentation",
  "categorizeByGroup": true,
  "categoryOrder": [
    "PGC Core",
    "Overlay Managers",
    "Commands",
    "Sigma",
    "TUI",
    "Security",
    "Services",
    "Utilities",
    "*"
  ],
  "plugin": [],
  "theme": "default",
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "sort": ["source-order"],
  "gitRevision": "main",
  "sourceLinkTemplate": "https://github.com/mirzahusadzic/cogx/blob/{gitRevision}/{path}#L{line}",
  "compilerOptions": {
    "skipLibCheck": true
  },
  "skipErrorChecking": true,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": true
  },
  "treatWarningsAsErrors": false,
  "searchInComments": true,
  "includeHierarchySummary": true,
  "lightHighlightTheme": "light-plus",
  "darkHighlightTheme": "dark-plus"
}
